If:
  PathMatch: .*\.c

CompileFlags:
  Add:
    [
      -D_DEFAULT_SOURCE,
      -D_REENTRANT,
      -I.,
      -I../../include,
      -I../include,
      -I/usr/include/jeff,
      -I/usr/include/luajit-2.1,
      -I/usr/local/include,
      -Iinclude,
      -L.,
      -L../lib,
      -L/usr/lib/jeff,
      -L/usr/local/lib,
      -L/usr/local/lib/jeff,
      -Llib,
      -Wall,
      -Wextra,
      -Wno-unused,
      -lc,
      -ljeff,
      -ljlua,
      -llua,
      -lluajit-5.1,
      -lm,
      -lpthread,
      -pthread,
      -std=gnu99,
      -xc,
    ]
  Remove: [-D_FORTIFY_SOURCE=*]
  CompilationDatabase: .
  Compiler: gcc

Index:
  Background: Build
  StandardLibrary: true

Completion:
  AllScopes: true

InlayHints:
  BlockEnd: true
  Designators: true
  Enabled: true
  ParameterNames: true
  DeducedTypes: true
  TypeNameLimit: 18

Hover:
  ShowAKA: true

