  UnusedIncludes: None

InlayHints:
  BlockEnd: Yes
  Designators: Yes
  Enabled: Yes
  ParameterNames: Yes
  DeducedTypes: No
  TypeNameLimit: 12

Hover:
  ShowAKA: Yes

