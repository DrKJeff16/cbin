If:
  PathMatch: .*\.c

CompileFlags:
  Add:
    [
      -D_DEFAULT_SOURCE,
      -D_GNU_SOURCE,
      -D_REENTRANT,
      -D_XOPEN_SOURCE=600,
      -I../../include,
      -I../include,
      -I/usr/include/SDL2,
      -I/usr/include/blkid,
      -I/usr/include/cairo,
      -I/usr/include/freetype2,
      -I/usr/include/fribidi,
      -I/usr/include/gdk-pixbuf-2.0,
      -I/usr/include/glib-2.0,
      -I/usr/include/graphene-1.0,
      -I/usr/include/gtk-4.0,
      -I/usr/include/harfbuzz,
      -I/usr/include/libmount,
      -I/usr/include/libpng16,
      -I/usr/include/pango-1.0,
      -I/usr/include/pixman-1,
      -I/usr/include/sysprof-6,
      -I/usr/lib/glib-2.0/include,
      -I/usr/lib/graphene-1.0/include,
      -I/usr/local/include,
      -Iinclude,
      -L.,
      -L../lib,
      -L/usr/local/lib,
      -Llib,
      -Wall,
      -lc,
      -lEGL,
      -lGL,
      -lGLEW,
      -lGLU,
      -lOpenGL,
      -lSDL2,
      -lSDL2_image,
      -lX11,
      -lXi,
      -lXrandr,
      -lcairo,
      -lcairo-gobject,
      -ldie,
      -ldl,
      -lerr,
      -lgdk_pixbuf-2.0,
      -lgio-2.0,
      -lglfw3,
      -lglib-2.0,
      -lglut,
      -lgobject-2.0,
      -lgraphene-1.0,
      -lgtk-4,
      -lharfbuzz,
      -lharfbuzz,
      -ljdie,
      -ljerr,
      -ljlog,
      -ljoperators,
      -ljstring,
      -llua,
      -lluajit-5.1,
      -lm,
      -lncursesw,
      -lpango-1.0,
      -lpangocairo-1.0,
      -lpthread,
      -lvulkan,
      -pthread,
      -std=c99,
    ]
  Remove: [-D_FORTIFY_SOURCE=*]
  Compiler: gcc

Index:
  Background: Build
  StandardLibrary: No

Completion:
  AllScopes: Yes

Diagnostics:
